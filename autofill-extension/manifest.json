{
  "manifest_version": 3,
  "name": "Flight Booker Autofill",
  "description": "Autofills passenger and contact info on booking pages.",
  "version": "1.5",
  "action": {
    "default_title": "Autofill"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://dshb.gth.com.ua/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ryanair.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "ryanair.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.wizzair.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "wizzair.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.hotelston.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "hotelston.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://app.hotelbeds.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "hotelbeds.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://b2bdirect.itravex.es/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "itravex.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://b2dmc.w2m.travel/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "w2m.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://bedbank.viajesolympia.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "viajesolympia.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.kiwi.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "kiwi.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.chartershop.com.ua/*",
        "*://*.chartershop.eu/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "chartershop.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.luxurytraveldmc.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "luxurytraveldmc.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.goglobal.travel/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "goglobal.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://book.webbeds.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "webbeds.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.tbohotels.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "tbohotels.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "http://smartsys.dyndns.biz/*",
        "https://smartsys.dyndns.biz/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "smartsys.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.joyce-tours.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "joyce-tours.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://online.khalidiah.ae/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "khalidiah.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://mresort.toursupport.ru/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "mresort.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.drct.aero/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "drct.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://*.ryanair.com/*",
        "*://*.wizzair.com/*",
        "*://*.hotelston.com/*",
        "*://app.hotelbeds.com/*",
        "*://*.chartershop.com.ua/*",
        "*://*.chartershop.eu/*",
        "*://*.luxurytraveldmc.com/*",
        "*://*.joyce-tours.com/*",
        "*://online.khalidiah.ae/*",
        "*://smartsys.dyndns.biz/*",
        "*://mresort.toursupport.ru/*",
        "*://book.webbeds.com/*",
        "*://*.drct.aero/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "common.js",
        "generic.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}